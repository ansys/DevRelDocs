{
  "metadata": [],
  "build": {
    "content": [
      {
        "files": [
          "api/**.md",
          "api/toc.yml"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/toc.yml"
        ],
        "exclude": [
            "_site/**"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Speos NX API Documentation",
      "_appFooter": "Speos NX API Reference",
      "_enableSearch": true
    },
    "template": [
      "default",
      "modern"
    ]
  }
}